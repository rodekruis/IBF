{
  "name": "api-service",
  "author": "IBF",
  "homepage": "https://github.com/rodekruis/IBF/",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/rodekruis/IBF.git",
    "directory": "services/api-service"
  },
  "main": "index.mjs",
  "scripts": {
    "lint": "eslint \"**/*.{ts,js}\"",
    "fix": "npm run lint -- --fix",
    "build": "nest build --path=tsconfig.json",
    "start": "GLOBAL_IBF_VERSION=$(test -f \"VERSION.txt\" && cat VERSION.txt || echo $GLOBAL_IBF_VERSION)  node dist/main.js",
    "start:dev": "nodemon --config nodemon.json --watch src/**/*.json --watch src/**/*.csv --exec npm run start:dev:nest",
    "start:dev:collect-coverage": "./node_modules/.bin/nyc npm run start:dev:nest",
    "start:dev:nest": "nest start --require=tsconfig-paths/register --debug=0.0.0.0:9229 --watch --path=tsconfig.json",
    "prestart": "npm run prisma:prepare",
    "prestart:dev": "npm install --no-save && npm run prisma:prepare",
    "prestart:dev:collect-coverage": "npm install --no-save && npm run prisma:prepare",
    "seed:dev": "ts-node --require=tsconfig-paths/register src/scripts seed-dev",
    "test": "echo 'No automated tests. Run `npm run test:integration:all` to run API-tests locally. Run `npm run test:unit:all` to run unit-tests locally.' && exit 0",
    "test:unit:all": "jest --runInBand --config=jest.unit.config.mjs",
    "test:unit:watch": "npm run test:unit:all -- --watchAll",
    "test:unit:coverage": "npm run test:unit:all -- --coverage --coverageDirectory=coverage/unit",
    "test:integration:local": "node --env-file-if-exists=../.env --require=tsconfig-paths/register --expose-gc node_modules/.bin/jest --config=jest.integration.config.mjs --runInBand --detectOpenHandles --logHeapUsage",
    "test:integration:all": "node --require=tsconfig-paths/register --expose-gc node_modules/.bin/jest --config=jest.integration.config.mjs --runInBand --detectOpenHandles --logHeapUsage",
    "test:integration:debug": "node --inspect-brk=0.0.0.0:9231 --require=tsconfig-paths/register --expose-gc node_modules/.bin/jest --config=jest.integration.config.mjs --runInBand --detectOpenHandles --logHeapUsage",
    "test:integration:watch": "npm run test:integration:all -- --watchAll",
    "coverage:report:unit": "echo 'This is generated with the test:unit:coverage script.' && exit 1",
    "coverage:report:integration": "nyc report --report-dir coverage/integration --reporter=lcov --reporter=text",
    "coverage:report:combined:merge": "istanbul-merge --out coverage/combined/coverage.json coverage/integration/coverage-final.json coverage/unit/coverage-final.json",
    "coverage:report:combined:generate": "nyc report -t coverage/combined --report-dir coverage/combined --reporter=lcov --reporter=text",
    "coverage:report:combined": "npm run coverage:report:combined:merge && npm run coverage:report:combined:generate",
    "coverage:open:unit": "open coverage/unit/lcov-report/index.html",
    "coverage:open:integration": "open coverage/integration/lcov-report/index.html",
    "coverage:open:combined": "open coverage/combined/lcov-report/index.html",
    "prisma": "npx prisma --config=src/prisma/prisma.config.ts",
    "prisma:generate": "npm run prisma generate",
    "prisma:migrate:dev": "npm run prisma migrate dev -- --name",
    "prisma:migrate:deploy": "npm run prisma migrate deploy",
    "prisma:prepare": "npm run prisma:generate && npm run prisma:migrate:deploy",
    "test:migration": "npm run prisma migrate status --exit-code",
    "knip": "knip",
    "knip:fix": "knip --fix-type files,exports,types",
    "typecheck": "tsc --noEmit",
    "typecheck:dev": "tsc --noEmit --watch",
    "install:dependencies-for-portal": "npm install"
  },
  "private": true,
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.13",
    "@nestjs/core": "^11.1.13",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.13",
    "@nestjs/swagger": "^11.2.6",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/testing": "^11.1.13",
    "@nestjs/throttler": "^6.5.0",
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "@t3-oss/env-core": "^0.13.10",
    "@types/express": "^5.0.6",
    "@types/he": "^1.2.3",
    "@types/jsonwebtoken": "^9.0.10",
    "applicationinsights": "^2.9.8",
    "axios": "^1.13.4",
    "body-parser": "^2.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cookie-parser": "^1.4.7",
    "he": "^1.2.0",
    "jsonwebtoken": "^9.0.3",
    "lodash": "^4.17.23",
    "nestjs-spelunker": "^1.3.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "ufo": "^1.6.2",
    "validator": "^13.15.26",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@automock/adapters.nestjs": "^2.1.0",
    "@compodoc/compodoc": "^1.1.32",
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.14",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.8",
    "@types/cookie-parser": "^1.4.10",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.23",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.x",
    "@types/passport-jwt": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-custom-rules": "file:./eslint-plugin-custom-rules/",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^29.12.2",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "istanbul-merge": "^2.0.0",
    "jest": "^30.2.0",
    "jest-ci-spec-reporter": "^1.0.4",
    "knip": "^5.83.0",
    "nodemon": "^3.1.11",
    "nyc": "^17.1.0",
    "prettier": "^3.7.4",
    "prisma": "^7.4.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6"
  }
}
